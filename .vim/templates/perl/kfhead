
###############################################################################

=head2 FNAME() 

laedt die Config von cpcom ein und einige Umgebungsvariablen ein

B<In:>

	none

B<Out:>

	none

B<Return:>

	none

=cut
###############################################################################
sub FNAME {
	my ( $var ) = @_;
	my ( $ret, $msg )	= ( M_OK, "" );

	_DBG($dbg, "{FNAME( )");

	if ( $var != M_OK )  _DBGERR( $dbg, M_ERR_ARG );

DONE:
	_DBGRETURN($dbg, $ret );
}

